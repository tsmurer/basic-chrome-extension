{
    "manifest_version": 3,
    "name": "Minimal extension",
    "version": "1.0",
    "description": "A minimal chrome extension to start from",
    "permissions": [
        "activeTab",
        "contextMenus",
        "storage"
      ],
    "host_permissions": [
        "https://www.developer.chrome.com/*"
    ],
    "optional_host_permissions":[
        "https://*/*",
        "http://*/*"
    ],
    "background": {
        "service_worker": "service-worker.js"
    }
  }